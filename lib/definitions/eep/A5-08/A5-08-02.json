{
  "eep": "A5-08-02",
  "rorg_number": "0xA5",
  "telegram": "4BS",
  "func_number": "0x08",
  "type_number": "0x02",
  "type_title": "Range 0lx to 1020lx, 0°C to +51°C and Occupancy Button",
  "case": {
    "datafield": [
      {
        "data": "Occupancy Button pressed",
        "shortcut": "OCC",
        "bitoffs": "31",
        "bitsize": "1",
        "iobroker": {
          "role": "switch",
          "type": "boolean",
          "read": true,
          "write": false
        },
        "value": {
          "==": [{"var": "value"}, 1]
        }
      },
      {
        "data": "Presence",
        "shortcut": "PIRS",
        "bitoffs": "30",
        "bitsize": "1",
        "iobroker": {
          "role": "sensor.motion",
          "type": "boolean",
          "read": true,
          "write": false
        },
        "value": {
          "==": [{"var": "value"}, 0]
        }
      },
      {
        "data": "Temperature 0...51°C",
        "shortcut": "TMP",
        "bitoffs": "16",
        "bitsize": "8",
        "iobroker": {
          "role": "value.temperature",
          "type": "number",
          "read": true,
          "write": false,
          "unit": "°C"
        },
        "value": {
          "*": [{
            "+": [
              { "-": [{"var": "value"}, 0] },
              0
            ]}, 0.2]
        },
        "decimals": 2
      },
      {
        "data": "Illumination 0...1020lx",
        "shortcut": "ILL",
        "bitoffs": "8",
        "bitsize": "8",
        "iobroker": {
          "role": "value.brightness",
          "type": "number",
          "read": true,
          "write": false,
          "unit": "lx"
        },
        "value": {
          "*": [{
            "+": [
              { "-": [{"var": "value"}, 0] },
              0
            ]}, 4]
        }
      },
      {
        "data": "Supply voltage 0…5.1V",
        "shortcut": "SVC",
        "bitoffs": "0",
        "bitsize": "8",
        "iobroker": {
          "role": "value.battery",
          "type": "number",
          "read": true,
          "write": false,
          "unit": "V"
        },
        "value": {
          "*": [{
            "+": [
              { "-": [{"var": "value"}, 0] },
              0
            ]}, 0.02]
        },
        "decimals": 2
      }
    ]
  }
}
